<%- include('../partials/header') %>

<h1>Tickets</h1>
<table>
    <thead>
      <tr>
        <th>Airport</th>
        <th>Arrival</th>
      </tr>
    </thead>
    <tbody>
      <% ticket.forEach( t => { %>
        <tr>
          <td><%= t.seat %></td>
          <td><%= t.price %></td>
          <td><%= t.flight %></td>
        </tr>
        <% }); %>
    </tbody>
</table>

<%- include('../partials/footer') %>
