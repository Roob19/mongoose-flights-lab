<%- include('../partials/header') %>
<h1>Destinations</h1>
<table>
    <thead>
      <tr>
        <th>Airport</th>
        <th>Arrival</th>
      </tr>
    </thead>
    <tbody>
      <% destination.forEach( d => { %>
        <tr>
          <td><%= d.airport %></td>
          <td><%= d.arrival %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
<%- include('../partials/footer') %>